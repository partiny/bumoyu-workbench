{
  "name": "bumoyu-workbench",
  "version": "1.1.1",
  "description": "不摸鱼的前端工程",
  "license": "MIT",
  "author": "Partiny <partiny@163.com>",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "prod": "vite --mode prod",
    "build": "run-p type-check build-only:prod",
    "preview": "vite preview",
    "build-only:prod": "vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "commit": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@unocss/reset": "^0.64.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.8",
    "dayjs": "^1.11.13",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@unocss/preset-rem-to-px": "^0.64.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "sass-embedded": "^1.83.3",
    "standard-version": "^9.5.0",
    "typescript": "~5.6.3",
    "unocss": "^0.64.1",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
